{
  "name": "@marblism/remix",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "init": "pnpm install && pnpm run docker:init ; pnpm run env:init ; pnpm run crud:sync && pnpm run database:sync:dev && pnpm run database:seed",
    "dev": "if exist build (del /s /q build) && if exist node_modules\\.vite (rmdir /s /q node_modules\\.vite) && tsx server/index.ts",
    "debug": "NODE_OPTIONS=--inspect-brk tsx server/index.ts",
    "start": "cross-env PORT=8099 NODE_ENV=production node ./build/server/index.js",
    "build": "remix vite:build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "check": "pnpm run check:code ; pnpm run check:models",
    "check:code": "tsc ; eslint . --ext .js,.jsx,.ts,.tsx",
    "check:models": "zenstack check",
    "docker:init": "docker compose -p marblism-app up -d",
    "crud:sync": "zenstack generate",
    "database:sync": "prisma db push",
    "database:sync:dev": "prisma db push --accept-data-loss",
    "database:seed": "tsx ./prisma/seed/mockUser .ts && tsx ./prisma/seed/mockData.ts ",
    "database:reset": "prisma db push --force-reset && pnpm run database:seed",
    "env:init": "node scripts/env.js",
    "format": "pnpm run format:code ; pnpm run format:models",
    "format:code": "prettier --write \"app/!(*.marblism)/**/*.{ts,tsx}\"",
    "format:models": "node scripts/format.models.js"
  },
  "dependencies": {
    "@ant-design/icons": "5.5.1",
    "@aws-sdk/client-s3": "^3.733.0",
    "@aws-sdk/s3-request-presigner": "^3.733.0",
    "@remix-pwa/push": "^2.10.3",
    "@remix-run/express": "^2.15.2",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@zenstackhq/runtime": "^2.11.4",
    "@zenstackhq/trpc": "^2.11.4",
    "antd": "5.21.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.5",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "isbot": "^4.4.0",
    "jsonwebtoken": "8.5.1",
    "jsonwebtoken-esm": "1.0.5",
    "morgan": "^1.10.0",
    "node-mailjet": "^6.0.6",
    "nodemailer": "^6.10.0",
    "openai": "^4.80.1",
    "papaparse": "^5.5.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "postcss": "^8.5.1",
    "posthog-js": "^1.207.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^2.15.0",
    "remix-pwa": "^4.0.5",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.6.0",
    "tsx": "^4.19.2",
    "uuid": "^10.0.0",
    "zenstack": "2.6.2",
    "zod": "^3.24.1",
    "zod-form-data": "^2.0.5"
  },
  "devDependencies": {
    "@marblism/eslint-plugin": "^1.0.0",
    "@prisma/client": "^5.22.0",
    "@remix-pwa/dev": "^3.1.0",
    "@remix-pwa/sw": "^3.0.10",
    "@remix-pwa/worker-runtime": "^2.1.4",
    "@remix-run/dev": "^2.15.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prisma": "^5.22.0",
    "sass-embedded": "^1.83.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vite-tsconfig-paths": "^5.1.4",
    "web-push": "^3.6.7"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "zenstack": {
    "schema": "./models/schema.zmodel"
  }
}
